    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "framework7": "^6.3.16",
    "framework7-icons": "^5.0.5",
    "framework7-svelte": "^6.3.16",
    "freetype2": "^1.0.6",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.0",
    "json-rpc-2.0": "^0.2.16",
    "material-icons": "^1.10.8",
